#!/bin/bash
if [[ -z $1 ]]; then
	BLACK="$HOME/code/gogo/gogo -gtp -go -stats -gfx -t 10 -c 0.1 -log gogo.log -amaf -neighbors -ancestor -seed -book -bfile $HOME/code/gogo/book.gob -cutoff 0.4"
else 
	BLACK="$HOME/code/gogo/gogo -gtp -go -stats -gfx -t 20 -e 50 -log gogo.log -amaf -seed -neighbors -ancestor -pat -tenuki -pfile $1 -c 0.1 -cutoff 0.4"
fi
WHITE="$HOME/code/gnugo-3.8/interface/gnugo --mode gtp --capture-all-dead --chinese-rules --never-resign --level 10"
gogui-twogtp \
	-black "gogui-display \"$BLACK\"" \
	-white "$WHITE" \
	-games 1 -size 9 -sgffile gnugo_gfx -auto -komi 6.5
