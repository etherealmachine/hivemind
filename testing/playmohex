#!/bin/bash
MOHEX="$HOME/code/benzene-0.9.0/src/mohex/mohex"
GOGO="$HOME/code/gogo/gogo -gtp -t=10 -var -amaf -k=550 -hex -pfile $1"
function play {
	BLACK=$GOGO
	WHITE=$MOHEX
	gogui-twogtp -auto -size 13 -black "$BLACK" -white "$WHITE" \
		-games 1 -sgffile mohex -verbose -alternate -openings openings13
}
play
